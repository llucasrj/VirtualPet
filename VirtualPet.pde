void setup()
{
size(400,400);
}
void draw()
{
//body
fill(100,100,100);
noStroke();
arc(180,270,200,220,PI/2,3*PI/2);
arc(220,270,200,220,3*PI/2,5*PI/2);
rect(150,163,100,215);
ellipse(200,120,190,180);
triangle(175,35,195,8,200,35);
triangle(185,35,205,12,210,35);
//arms
fill(100,100,100);
noStroke();
pushMatrix();
translate(65,200);
rotate(7*PI/4);
ellipse(0,0,80,140);
popMatrix();
pushMatrix();
translate(335,200);
rotate(5*PI/4);
ellipse(0,0,80,140);
popMatrix();
//belly
noStroke();
fill(255,255,255);
ellipse(200,270,180,190);
ellipse(200,150,130,90);
ellipse(160,120,60,130);
ellipse(240,120,60,130);
//beak and feet
fill(255,200,0);
arc(200,120,50,40,PI,4*PI/2);
arc(200,120,40,45,4*PI/2,6*PI/2);
pushMatrix();
translate(145,340);
ellipse(0,0,40,75);
rotate(11*PI/6);
ellipse(-10,0,40,75);
rotate(PI/3);
ellipse(10,0,40,75);
popMatrix();
pushMatrix();
translate(255,340);
ellipse(0,0,40,75);
rotate(11*PI/6);
ellipse(-10,0,40,75);
rotate(PI/3);
ellipse(10,0,40,75);
popMatrix();
//eyes
fill(0);
ellipse(160,93,40,45);
ellipse(240,93,40,45);
fill(255);
ellipse(165,85,10,10);
ellipse(245,85,10,10);
}
